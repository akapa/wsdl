{
  "name": "kapa/wsdl",
  "version": "0.0.1",
  "description": "Use WSDL Webservices, serialize/unserialize objects based on XSD",
  "main": "dist/wsdl.js",
  "dependencies": {
    "requirejs": "latest",
    "underscore": "latest",
    "xml": "bazmegakapa/kapa-xml",
    "Library": "bazmegakapa/kapa-Library",
    "primitiveSerializers": "bazmegakapa/kapa-primitiveSerializers",
    "primitiveUnserializers": "bazmegakapa/kapa-primitiveUnserializers",
    "objTools": "bazmegakapa/kapa-objTools"
  },
  "install": {
    "path": {
      "js": "js/lib"
    }
  },
  "keywords": [
    "wsdl",
    "xsd",
    "webservices",
    "serialization"
  ],
  "authors": [
    "Kapa"
  ],
  "license": "MIT",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ]
}
